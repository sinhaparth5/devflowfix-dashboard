{
  "name": "devflowfix",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "wasm:build": "cd wasm && wasm-pack build --target web --out-dir pkg",
    "wasm:build:dev": "cd wasm && wasm-pack build --target web --out-dir pkg --dev",
    "prebuild": "wasm-pack --version && npm run wasm:build || echo 'wasm-pack not found, using pre-built wasm/pkg'",
    "prestart": "wasm-pack --version && npm run wasm:build:dev || echo 'wasm-pack not found, using pre-built wasm/pkg'",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts5": "^5.13.5",
    "@amcharts/amcharts5-geodata": "^5.1.4",
    "@angular/cdk": "^21.0.6",
    "@angular/common": "^21.1.1",
    "@angular/compiler": "^21.1.1",
    "@angular/core": "^21.1.1",
    "@angular/forms": "^21.1.1",
    "@angular/platform-browser": "^21.1.1",
    "@angular/platform-browser-dynamic": "^21.1.1",
    "@angular/router": "^21.1.1",
    "@fullcalendar/angular": "^6.1.20",
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "@popperjs/core": "^2.11.8",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/prismjs": "^1.26.5",
    "apexcharts": "^5.3.2",
    "flatpickr": "^4.6.13",
    "ng-apexcharts": "^2.0.0",
    "ng-otp-input": "^2.0.9",
    "postcss": "^8.5.6",
    "prismjs": "^1.30.0",
    "rxjs": "~7.8.0",
    "swiper": "^11.2.10",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0",
    "devflowfix-wasm": "file:wasm/pkg"
  },
  "devDependencies": {
    "@angular/build": "^21.1.1",
    "@angular/cli": "^21.1.1",
    "@angular/compiler-cli": "^21.1.1",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^27.4.0",
    "playwright": "^1.58.0",
    "postcss": "^8.5.3",
    "typescript": "~5.9.3",
    "vitest": "^4.0.18"
  }
}
